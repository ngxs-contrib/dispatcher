{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "emitter-e2e": {
            "root": "",
            "sourceRoot": "e2e",
            "projectType": "application",
            "prefix": "app",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/emitter-e2e",
                        "index": "e2e/index.html",
                        "main": "e2e/main.ts",
                        "polyfills": "e2e/polyfills.ts",
                        "tsConfig": "e2e/tsconfig.app.json",
                        "assets": ["e2e/favicon.ico", "e2e/assets"],
                        "styles": ["e2e/styles.css"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [{
                                "replace": "e2e/environments/environment.ts",
                                "with": "e2e/environments/environment.prod.ts"
                            }],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        }
                    }
                },

                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "emitter-e2e:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "emitter-e2e:build:production"
                        }
                    }
                },

                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "emitter-e2e:build"
                    }
                },

                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "e2e/test.ts",
                        "polyfills": "e2e/polyfills.ts",
                        "tsConfig": "e2e/tsconfig.spec.json",
                        "karmaConfig": "e2e/karma.conf.js",
                        "styles": ["e2e/styles.css"],
                        "scripts": [],
                        "assets": ["e2e/favicon.ico", "e2e/assets"]
                    }
                },

                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "e2e/tsconfig.app.json",
                            "e2e/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },

        "emitter": {
            "root": "./",
            "sourceRoot": "./",
            "projectType": "library",
            "prefix": "emitter",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "src/tsconfig.lib.json",
                        "project": "src/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "project": "src/ng-package.prod.json"
                        }
                    }
                },

                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js"
                    }
                },

                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.lib.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },

    "defaultProject": "emitter-e2e"
}
