{
    "$schema": "../node_modules/ng-packagr/package.schema.json",
    "name": "@ngxs-labs/emitter",
    "version": "1.9.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ngxs-labs/emitter.git"
    },
    "license": "MIT",
    "homepage": "https://github.com/ngxs-labs/emitter#readme",
    "bugs": {
        "url": "https://github.com/ngxs-labs/emitter/issues"
    },
    "keywords": [
        "ngxs",
        "redux",
        "store"
    ],
    "sideEffects": true,
    "peerDependencies": {
        "@angular/core": "^7.0.0",
        "@ngxs/store": ">=3.3.4"
    },
    "ngPackage": {
        "lib": {
            "flatModuleFile": "ngxs-labs-emitter",
            "entryFile": "src/index.ts",
            "umdModuleIds": {
                "@ngxs/store": "ngxs-store"
            }
        },
        "dest": "../dist/emitter"
    }
}
